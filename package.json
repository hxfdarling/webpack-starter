{
  "name": "webpack-starter",
  "version": "1.0.0",
  "description": "webpack demo project",
  "main": "index.js",
  "author": "zman",
  "license": "MIT",
  "scripts": {
    "proxy": "node proxy/index.js",
    "start": "webpack-dev-server --progress",
    "clean": "rimraf dist",
    "build": "npm run lint && npm run test && npm run clean && webpack --config build/webpack.prod.conf.js --progress --profile --colors",
    "build.notest": "npm run clean && webpack --config build/webpack.prod.conf.js --progress --profile --colors",
    "build.dev": "npm run clean && cross-env BUILD_ENV=development webpack --config build/webpack.prod.conf.js --progress --profile --colors",
    "build.dll": "rimraf dll && webpack --config build/webpack.dll.conf.js --progress --profile --colors",
    "lint": "eslint --format node_modules/eslint-friendly-formatter --ext .js,.vue js test/unit/specs test/e2e/specs",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "unit.w": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js",
    "test": "npm run unit"
  },
  "dependencies": {
    "animate.css": "^3.5.2",
    "clone": "^2.1.1",
    "deep-equal": "^1.0.1",
    "extend": "^3.0.1",
